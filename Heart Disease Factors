    "#adding new column to the entire dataset\n",
    "\n",
    "#function to check cholesterol health\n",
    "def checkchol(list):\n",
    "    chollist = []\n",
    "    for x in list:\n",
    "        if x >= 240:\n",
    "            chollist.append(\"High\")\n",
    "        elif 239 < x < 200:\n",
    "            chollist.append(\"Borderline\")\n",
    "        else:\n",
    "            chollist.append(\"Normal\")\n",
    "    return chollist\n",
    "\n",
    "heart_disease['Chol Health'] = checkchol(heart_disease['Cholesterol'])\n",
    "\n",
    "#added another column\n",
    "heart_disease['HeartDiseaseValues'] = heart_disease['HeartDisease'].astype(bool)\n",
    "\n",
    "heart_disease.head()"
